{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "export NODE_ENV=dev && node ./dist/server.js",
    "start:watch": "export NODE_ENV=dev && ./node_modules/.bin/nodemon ./src/server.ts",
    "watch": "export NODE_ENV=dev && tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "test": "export NODE_ENV=test && db-migrate db:create store_front_test && db-migrate --env test up && jasmine-ts && db-migrate db:drop store_front_test",
    "build": "tsc",
    "dev:start": "export NODE_ENV=dev && ts-node src/server.ts",
    "migrate": "./node_modules/.bin/db-migrate --env dev up"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/pg": "^7.14.7",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "nodemon": "^2.0.7"
  }
}
